{
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "none",
  "importOrder": [
    "^react",
    "^react(/.*|$)",
    "<THIRD_PARTY_MODULES>",
    "^(client|context|store|query|consts|hooks|utils|api)(/.*|$)",
    "^(components|pages|oldComponents|oldPages|src)(/.*|$)",
    "^(assets|stories)(/.*|$)",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
